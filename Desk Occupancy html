<body>
    <iframe id="lookerStudio" src="https://lookerstudio.google.com/embed/reporting/f23b30da-af8c-418d-9190-797509292f32/page/Nho7D"></iframe>
    <script>
        const pageUrls = [
            'https://lookerstudio.google.com/embed/reporting/f23b30da-af8c-418d-9190-797509292f32/page/Nho7D',
            'https://lookerstudio.google.com/embed/reporting/f23b30da-af8c-418d-9190-797509292f32/page/p_t2kk026ujd',
            'https://lookerstudio.google.com/embed/reporting/f23b30da-af8c-418d-9190-797509292f32/page/p_lmsdo94tjd',
            'https://lookerstudio.google.com/embed/reporting/f23b30da-af8c-418d-9190-797509292f32/page/p_vflpqulwjd',
            'https://lookerstudio.google.com/embed/reporting/f23b30da-af8c-418d-9190-797509292f32/page/p_8ocgbr8ujd'
        ];
        let currentPage = 0;

        function nextPage() {
            currentPage = (currentPage + 1) % pageUrls.length;
            document.getElementById('lookerStudio').src = pageUrls[currentPage];
        }

        setInterval(nextPage, 10000); // Change page every 10 seconds
    </script>
